{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/Experiment-11_ReactJS_Student-management/(.*)",
      "dest": "/Experiment-11_ReactJS_Student-management/build/$1"
    },
    {
      "src": "/Experiment-12_ReactJS_Weather-app/(.*)",
      "dest": "/Experiment-12_ReactJS_Weather-app/weather-app/public/$1"
    },
    {
      "src": "/Experiment-13_ReactJS_Todo-app/(.*)",
      "dest": "/Experiment-13_ReactJS_Todo-app/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/Experiment-11_ReactJS_Student-management/static/(.*)",
      "destination": "/Experiment-11_ReactJS_Student-management/build/static/$1"
    },
    {
      "source": "/Experiment-13_ReactJS_Todo-app/static/(.*)",
      "destination": "/Experiment-13_ReactJS_Todo-app/build/static/$1"
    }
  ]
}
